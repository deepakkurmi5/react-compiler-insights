{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "main",
  "targetDefaults": {
    "build": {
      "cache": true,
      "outputs": [
        "{projectRoot}/dist"
      ],
      "dependsOn": [
        "lint",
        "typecheck"
      ]
    },
    "lint": {
      "cache": true
    },
    "typecheck": {
      "cache": true
    }
  },
  "release": {
    "projects": [
      "packages/solana-client"
    ]
  }
}